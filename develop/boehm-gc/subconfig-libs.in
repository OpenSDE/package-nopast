if pkgcheck boehm-gc X
then
	comment '--- Boehm-Demers-Weiser conservative garbage collector'

	pkgout
	while read x y; do
		bool "Use it in Objective-C (gcc-$y)" SDECFG_PKG_BOEHMGC_OBJC${x#gcc} 0
		bool "Use it in Java (gcc-$y)" SDECFG_PKG_BOEHMGC_JAVA${x#gcc} 0
		#comment ' '
	done < <( grep '^X .* gcc.* ' $cfgtmpdir/config/packages | grep -v cross | cut -d' ' -f 5,6 )

fi
